{
  "name": "jest-crashing-jenkins",
  "description": "Jest crashing jenkins",
  "version": "0.0.1",
  "main": "dist/jest-crashing-jenkins.js",
  "files": [
    "dist/",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "jest-crashing-jenkins"
  },
  "engines": {
    "npm": ">=3",
    "node": ">=5"
  },
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "node $(pwd)/internals/scripts/analyze.js",
    "npmcheckversion": "node $(pwd)/internals/scripts/npmcheckversion.js",
    "prebuild": "npm run build:clean && npm run test",
    "build:dist": "cross-env NODE_ENV=production webpack --config $(pwd)/internals/webpack/webpack.dist.babel.js --color -p --progress",
    "build:lib": "cross-env NODE_ENV=production babel -d lib/ src/",
    "build:watch": "cross-env NODE_ENV=production webpack --config $(pwd)/internals/webpack/webpack.dist.babel.js --color -p --progress --watch",
    "build:clean": "npm run test:clean && rimraf $(pwd)/dist $(pwd)/lib",
    "dist": "npm run prebuild && npm run build:lib && npm run build:dist",
    "presetup": "npm i chalk shelljs",
    "clean": "shjs $(pwd)/internals/scripts/clean.js",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path $(pwd)/.gitignore --ignore-path $(pwd)/.eslintignore --ignore-pattern internals/scripts",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "pretest": "npm run test:clean && npm run lint",
    "tdd": "npm run test:watch",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "lint-staged": {
    "*.js": "lint:eslint"
  },
  "pre-commit": [
    "lint:staged"
  ],
  "babel": {
    "presets": [
      [
        "env",
        {
          "es2015": {
            "modules": false
          }
        }
      ],
      "react",
      "stage-0"
    ],
    "env": {
      "production": {
        "only": [
          "src"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements",
          "array-includes",
          "styled-components"
        ]
      },
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs",
          "dynamic-import-node"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "plugins": [
      "react",
      "jsx-a11y"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "arrow-parens": [
        "error",
        "always"
      ],
      "arrow-body-style": [
        2,
        "as-needed"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "import/imports-first": 0,
      "import/newline-after-import": 0,
      "import/no-dynamic-require": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-named-as-default": 0,
      "import/no-unresolved": 2,
      "import/prefer-default-export": 0,
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "jsx-a11y/aria-props": 2,
      "jsx-a11y/heading-has-content": 0,
      "jsx-a11y/href-no-hash": 2,
      "jsx-a11y/label-has-for": 2,
      "jsx-a11y/mouse-events-have-key-events": 2,
      "jsx-a11y/role-has-required-aria-props": 2,
      "jsx-a11y/role-supports-aria-props": 2,
      "max-len": 0,
      "newline-per-chained-call": 0,
      "no-confusing-arrow": 0,
      "no-console": 1,
      "no-use-before-define": 0,
      "prefer-template": 2,
      "class-methods-use-this": 0,
      "react/forbid-prop-types": 0,
      "react/jsx-first-prop-new-line": [
        2,
        "multiline"
      ],
      "react/jsx-filename-extension": 0,
      "react/jsx-no-target-blank": 0,
      "react/require-extension": 0,
      "react/self-closing-comp": 0,
      "require-yield": 0,
      "import/no-webpack-loader-syntax": 0
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./internals/webpack/webpack.dist.babel.js"
        }
      }
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.test.{js,jsx}",
      "!src/*/RbGenerated*/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 92,
        "branches": 92,
        "functions": 92,
        "lines": 92
      }
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/mocks/cssModule.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/image.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/internals/testing/test-bundler.js",
    "testRegex": "tests/.*\\.test\\.js$",
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/lib/"
    ],
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "peerDependencies": {
    "react": "^15.5.4",
    "styled-components": "^2.0.0-17",
    "react-transition-group": "^1.1.1"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "color": "1.0.3",
    "lodash.omit": "^4.5.0",
    "prop-types": "^15.5.8",
    "tether": "1.3.4"
  },
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-array-includes": "2.0.3",
    "babel-plugin-dynamic-import-node": "1.0.0",
    "babel-plugin-react-intl": "2.2.0",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-styled-components": "1.1.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.22.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "0.2.11",
    "babel-polyfill": "6.22.0",
    "babel-preset-env": "1.2.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.22.0",
    "circular-dependency-plugin": "2.0.0",
    "coveralls": "2.11.15",
    "cross-env": "3.1.4",
    "css-loader": "0.26.1",
    "enzyme": "2.8.2",
    "eslint": "3.11.1",
    "eslint-config-airbnb": "13.0.0",
    "eslint-config-airbnb-base": "10.0.1",
    "eslint-import-resolver-webpack": "0.8.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.7.1",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.6.3",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "image-webpack-loader": "2.0.0",
    "immutable": "3.8.1",
    "imports-loader": "0.6.5",
    "jest-cli": "19.0.2",
    "jest-sonar-reporter": "1.1.1",
    "lint-staged": "3.2.1",
    "ngrok": "2.2.4",
    "node-plop": "0.5.4",
    "null-loader": "0.1.1",
    "plop": "1.7.3",
    "pre-commit": "1.1.3",
    "react": "15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "15.4.2",
    "react-test-renderer": "15.5.4",
    "react-transition-group": "1.1.1",
    "rimraf": "2.5.4",
    "shelljs": "0.7.5",
    "sinon": "2.0.0-pre",
    "sonar-scanner": "1.0.1",
    "style-loader": "0.13.1",
    "styled-components": "2.0.0-17",
    "url-loader": "0.5.7",
    "webpack": "2.4.1",
    "webpack-dev-middleware": "1.10.2",
    "webpack-hot-middleware": "2.18.0"
  }
}
